[Desktop Entry]
Type=Application
Name=KeePassXC (autologin)
Comment=Launches KeePassXC and logs in if password is in the keyring
Exec=/bin/sh -c "$HOME/.local/bin/launch-keepassxc"
Icon=keepassxc
X-GNOME-Autostart-enabled=true
X-GNOME-Autostart-Delay=10
Hidden=false
NoDisplay=false
Name[en_US]=Script - KeePassXC Auto-login
